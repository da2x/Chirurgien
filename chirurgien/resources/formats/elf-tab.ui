          <property name="row-span">1</property>
          <property name="column-span">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label" translatable="yes">Details</property>
        <property name="margin-top">10</property>
        <property name="margin-bottom">5</property>
        <layout>
          <property name="row">6</property>
          <property name="column">0</property>
          <property name="row-span">1</property>
          <property name="column-span">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow">
        <property name="hexpand">t</property>
        <property name="vexpand">t</property>
        <property name="hscrollbar-policy">GTK_POLICY_NEVER</property>
        <child>
          <object class="GtkTextView">
            <property name="can-target">f</property>
            <property name="left-margin">10</property>
            <property name="right-margin">10</property>
            <property name="top-margin">7</property>
            <property name="bottom-margin">7</property>
            <property name="editable">f</property>
            <property name="cursor-visible">f</property>
            <property name="wrap-mode">GTK_WRAP_WORD</property>
            <property name="buffer">details</property>
          </object>
        </child>
        <layout>
          <property name="row">7</property>
          <property name="column">0</property>
          <property name="row-span">1</property>
          <property name="column-span">6</property>
        </layout>
      </object>
    </child>
  </object>
  <object class="GtkTextBuffer" id="details">
    <property name="text" translatable="yes"><![CDATA[The ABI version field is not interpreted
The SHT_SYMTAB_SHNDX section type is not recognized
The sh_info field of a section is not interpreted
ELF files with more than 65280 (FF00) sections are not supported

The following are also <span foreground="red">not recognized</span>:
    - Object file types in the reserved ranges:
        - FE00-FEFF (OS-specific)
        - FF00-FFFF (processor-specific)
    - Segment types in the reserved ranges:
        - 60000000-6FFFFFFF (OS-specific)
        - 70000000-7FFFFFFF (processor-specific)
    - Segment flags in the reserved bit masks
        - FF00000 (OS-specific)
        - F0000000 (processor-specific)
    - Section types in the reserved ranges:
        - 60000000-6FFFFFFF (OS-specific)
        - 70000000-7FFFFFFF (processor-specific)
        - 80000000-FFFFFFFF (application-specific)
    - Section flags in the reserved bit masks:
        - FF00000 (OS-specific)
        - F0000000 (processor-specific)]]></property>
  </object>
</interface>

